<div class="container-fluid container-top bg-covr" style="">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-3 col-md-6">
        <div class="ibox float-e-margins bg-frm-opcty">
    <div class="ibox-title">
        <h5>Iniciar sessão</h5>
    </div>
    <div class="ibox-content">
        <%= form_for(resource, as: resource_name,  url: session_path(resource_name),class:"form-horizontal") do |f| %>
            <br/>
            <div class="form-group row"><label class="col-lg-2 control-label">Email</label>

                <div class="col-lg-10"><%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control" %>
                </div>
            </div>
            <div class="form-group row"><label class="col-lg-2 control-label">Password</label>

                <div class="col-lg-10"><%= f.password_field :password, autocomplete: "off" , class:"form-control" %></div>
            </div>
            <div class="form-group row">
                <div class="col-lg-offset-2 col-lg-10">
                  <% if devise_mapping.rememberable? -%>
                    <div class="i-checks pull-left text-small">
                      <%= f.check_box :remember_me %>
                      <%= f.label :remember_me %>
                    </div>
                  <% end -%>
                  <a href="/users/password/new" class="btn-link pull-right btnlnk m-t-xs">Recuperar password</a>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-lg-offset-2 col-lg-10">
                  <%= f.submit "Entrar", class:"btn btn-primary" %>
                </div>
            </div>
            <hr>
            <div class="row">
              <div class="col-lg-offset-2 col-lg-10">
              <table>
                  
                  <table>
                    <tr>
                      <td>
                        <span>Ainda não tem conta registada?</span>
                      </td>
                       <td>
                         <a href="/users/sign_up" class="btn-link btnlnk m-l-md">Registar</a>
                      </td>
                    </tr>
                    
              </table>
              </div>
            </div>
        <% end %>
    </div>
  </div>
      </div>
    </div>
  </div>
</div>
  



