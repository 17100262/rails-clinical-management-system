<% if current_user.admin? %>
    <%= render 'form_old', clinical_case: @clinical_case%>
<% elsif current_user.user_type == 'OM' %>
    <%= render 'form_om', clinical_case: @clinical_case %>
<% elsif current_user.user_type == 'MGF' %>
    <%= render 'form_mgf', clinical_case: @clinical_case %>
<% end %>
