  <div class="nested-fields">
      <div class="form-group"><label class="col-lg-12 control-label">Data</label>
        <div class="col-lg-12"><%= f.text_field :date_of_imaging, class:"form-control date" %></div>
      </div>
      <div class="form-group">
        <div class="col-lg-12">
          <div class="i-checks"> <%= f.check_box(:mammography,{}, checked_value = "1", unchecked_value = "0") %>
            <label class="control-label">Mamografia</label>
          </div>
        </div>
        
        <div class="col-lg-12">
          <div class="i-checks"> <%= f.check_box(:eco_breast,{}, checked_value = "1", unchecked_value = "0") %>
            <label class="control-label">Ecomam√°ria</label>
          </div>
        </div>
        
        <div class="col-lg-12">
          <label class="control-label">Birads</label>
          <div class="i-checks"> <%= f.select(:birads,[1,2,3,4,5]) %>
            
          </div>
        </div>
        
      </div>
      <div class="form-group"><label class="col-lg-12 control-label">Resultado</label>
        <div class="col-lg-12"><%= f.text_area :result, class:"form-control" %></div>
      </div>
      <% if current_page?(new_clinical_case_path) %>
        <%= link_to_remove_association "Remover",f,class:"text-danger" %>
      <%end%>
  </div>