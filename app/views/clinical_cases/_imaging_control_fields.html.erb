  <div class="nested-fields">
      <div class="form-group"><label class="col-lg-12 control-label">Data</label>
        <div class="col-lg-12"><%= f.date_field :date_of_imaging, class:"form-control" %></div>
      </div>
      <div class="form-group">
        <div class="col-lg-12">
          <div class="i-checks"> <%= f.check_box(:mammography,{}, checked_value = "1", unchecked_value = "0") %>
            <label class="control-label">Metastização à Distância</label>
          </div>
        </div>
        <div class="col-lg-12">
          <div class="i-checks"> <%= f.check_box(:eco_breast,{}, checked_value = "1", unchecked_value = "0") %>
            <label class="control-label">Ecomamária</label>
          </div>
        </div>
      </div>
      <div class="form-group"><label class="col-lg-12 control-label">Resultado</label>
        <div class="col-lg-12"><%= f.text_area :result, class:"form-control" %></div>
      </div>
      <%= link_to_remove_association "Remover",f,class:"text-danger" %>
  </div>